		<products>
			<li>
				<thingDef>BarIronBeaufort</thingDef>
				<count>1</count>
			</li>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>IronLump</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
	<RecipeDef>
</Defs>
